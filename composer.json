{
    "name": "p2made/yii2-p2y2-things",
    "description": "A bunch of CSS & JS resources wrapped as Yii 2 AssetBundles &/or Widgets.",
    "type": "yii2-extension",
    "keywords": [
        "yii2", "yii", "extension", "PHP",
        "asset", "bundle", "widget",
        "menu", "metisMenu",
        "Bootstrap Social", "social",
        "dataTables", "data", "tables",
        "Flot", "Morris", "Morris.JS", "charts", "excanvas",
        "raphael", "graphics", "vector", "vector graphics",
        "SweetAlert", "alerts", "Holder.js", "placeholder", "placeholder-image",
        "moment.js", "time", "moment", "timezone", "moment-timezone",
        "FullCalendar", "calendar",
        "html5shiv", "Respond.JS"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Plowman",
            "email": "p2opensource@gmail.com",
            "homepage": "https://github.com/p2made"
        }
    ],

    "version": "1.0.12",
    "minimum-stability": "dev",

    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2": ">=2.0.7",
        "fxp/composer-asset-plugin": ">=v1.1.2",
        "bower-asset/animate.css": "3.5.1",
        "bower-asset/bootstrap-social": "4.12.0",
        "bower-asset/bootstrap-sweetalert": "0.4.5",
        "bower-asset/bootstrap-switch": "v3.3.2",
        "bower-asset/bootswatch": "v3.3.5",
        "bower-asset/datatables": "1.10.11",
        "bower-asset/flag-icon-css": "1.4.0",
        "bower-asset/holderjs": "v2.9.3",
        "bower-asset/jquery.fitvids": "v1.1.0",
        "bower-asset/metismenu": "v2.4.2",
        "bower-asset/sweetalert": "v1.1.3",
        "philippfrenzel/yii2fullcalendar": "2.5.0",
        "rmrevin/yii2-fontawesome": "2.13.*"
    },
    "autoload": {
        "psr-4": {
            "p2made\\": ""
        }
    }
}
